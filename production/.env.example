# PostgreSQL Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=CHANGEZ_CE_MOT_DE_PASSE_TRES_SECURISE
POSTGRES_DB=n8n

# Timezone
TZ=Europe/Brussels
GENERIC_TIMEZONE=Europe/Brussels

# n8n Domain Configuration
# IMPORTANT: Remplacez par votre nom de domaine réel
N8N_DOMAIN=n8n.votredomaine.com
N8N_PROTOCOL=https

# Authentification basique n8n (optionnel)
# Décommentez et configurez si vous souhaitez utiliser l'authentification basique
# N8N_BASIC_AUTH_USER=admin
# N8N_BASIC_AUTH_PASSWORD=votre_mot_de_passe_securise

# Certbot Configuration (pour génération du certificat SSL)
CERTBOT_EMAIL=votre.email@exemple.com